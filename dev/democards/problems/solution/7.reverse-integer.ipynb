    "using LeetCode, Test\n",
    "\n",
    "function reverse_integer(x::Int32)::Int\n",
    "    s = sign(x)\n",
    "    r = parse(Int, reverse(string(s * x)))\n",
    "    return r <= typemax(Int32) ? s * r : 0\n",
    "end"
   ],
