    "using LeetCode, Test\n",
    "\n",
    "function range_bitwise_and(m::Int, n::Int)::Int\n",
    "    i = 0\n",
    "    while m != n\n",
    "        m >>= 1\n",
    "        n >>= 1\n",
    "        i += 1\n",
    "    end\n",
    "    return n << i\n",
    "end"
   ],
